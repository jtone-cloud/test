e != 'info'\"\r\n        label=\"url\"\r\n        prop=\"url\"\r\n      >\r\n        <el-input\r\n          style=\"min-width: 200px; max-width: 600px\"\r\n          type=\"textarea\"\r\n          :rows=\"8\"\r\n          placeholder=\"url\"\r\n          v-model=\"ruleForm.url\"\r\n        >\r\n        </el-input>\r\n      </el-form-item>\r\n      <el-form-item\r\n        :style=\"{\r\n          width: '48%',\r\n          margin: '0 0 30px 0',\r\n          fontSize: 'inherit',\r\n          color: 'inherit',\r\n        }\"\r\n        v-else-if=\"ruleForm.url\"\r\n        label=\"url\"\r\n        prop=\"url\"\r\n      >\r\n        <span\r\n          :style=\"{\r\n            padding: '0 20px',\r\n            color: 'inherit',\r\n            background: '#fff',\r\n            display: 'inline-block',\r\n            width: '80%',\r\n            fontSize: '14px',\r\n            lineHeight: '40px',\r\n            fontWeight: '500',\r\n          }\"\r\n          >{{ ruleForm.url }}</span\r\n        >\r\n      </el-form-item>\r\n      <el-form-item\r\n        :style=\"{\r\n          padding: '0 10px',\r\n          margin: '30px 0',\r\n          alignItems: 'center',\r\n          textAlign: 'center',\r\n          display: 'flex',\r\n          width: '100%',\r\n          perspective: '320px',\r\n          '-webkitPerspective': '320px',\r\n          fontSize: '48px',\r\n          justifyContent: 'flex-end',\r\n        }\"\r\n        class=\"btn\"\r\n      >\r\n        <el-button\r\n          class=\"btn3\"\r\n          v-if=\"type != 'info'\"\r\n          type=\"success\"\r\n          @click=\"onSubmit\"\r\n        >\r\n          <span\r\n            class=\"icon iconfont icon-tijiao16\"\r\n            :style=\"{\r\n              margin: '0 2px',\r\n              fontSize: '18px',\r\n              color: 'inherit',\r\n              display: 'none',\r\n            }\"\r\n          ></span>\r\n          提交\r\n        </el-button>\r\n        <el-button\r\n          class=\"btn4\"